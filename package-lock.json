{
  "name": "npm-workspace-test",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "npm-workspace-test",
      "version": "1.0.0",
      "workspaces": [
        "my-module-*"
      ]
    },
    "my-module-a": {
      "version": "0.0.1"
